<div class="editArea">

  <input type="text" name="firstname" value="" id="projectTitleInput" placeholder="Project title...">
  <canvas id="mainCanvas" width="400" height="400" value="yes"></canvas>
  <input type="text" name="" value="" id="bgSearch">

  <div class="buttonsPanel">
    <button type="button" name="button" id="addStickerButton" class="greenRoundButton">Sticker</button>
    <button type="button" name="button" id="addBackgroundButton" class="greenRoundButton">Background</button>
    <button type="button" name="button" id="saveProjectButton" class="greenRoundButton">Save Project</button>
    <button type="button" name="button" id="saveButton" class="greenRoundButton">Download</button>
  </div>

  <div class="toolsPanel">
    <button type="button" name="button" id="layerUpButton" class="greenRoundButton">Layer Up</button>
    <button type="button" name="button" id="layerDownButton" class="greenRoundButton">Layer Down</button>
    <button type="button" name="button" id="cloneButton" class="greenRoundButton">Clone</button>
    <button type="button" name="button" id="deleteStickerButton" class="redRoundButton">Delete</button>
  </div>


</div>
